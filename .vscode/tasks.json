{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "pyuic",
            "type": "shell",
            "command": "pyuic5 ${fileDirname}\\${fileBasename} -o ${workspaceFolder}\\wdo\\${fileBasenameNoExtension}.py"
        },
        {
            "label": "pyrcc",
            "type": "shell",
            "command": "pyrcc5 ${fileDirname}\\${fileBasename} -o ${workspaceFolder}\\wdo\\${fileBasenameNoExtension}_rc.py",
        }
    ]
}